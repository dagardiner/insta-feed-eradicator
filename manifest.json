{
    "manifest_version": 2,
    "name": "Insta Feed Eradicator",
    "version": "0.9",
    "content_scripts": 
    [
        {
            "matches":
            [
                "https://www.instagram.com/*"
            ],
            "js":
            [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "browser_action":
    {
        "default_popup": "popup.html",
        "default_icon": 
        {
            "16": "icons/icon_16.png",
            "32": "icons/icon_32.png",
            "64": "icons/icon_64.png",
            "128": "icons/icon_128.png"
        }
    },
    "permissions": 
    [
        "activeTab",
        "declarativeContent",
        "storage"
    ]
}