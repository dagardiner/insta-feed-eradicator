{
    "manifest_version": 2,
    "name": "Insta Wall Eradicator",
    "version": "0.1",
    "content_scripts": 
    [
        {
            "matches":
            [
                "https://www.instagram.com/*"
            ],
            "js":
            [
                "content.js"
            ]
        }
    ],
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background":
    {
        "scripts": ["background.js"],
        "persistent" : false
    },
    "permissions": 
    [
        "activeTab",
        "declarativeContent",
        "storage"
    ]
}